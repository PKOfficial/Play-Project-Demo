# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
GET   /login                           controllers.Authorization.login
GET /failed                             controllers.Authorization.failed
POST   /submit                          controllers.Authorization.submit
#GET     /                           controllers.TestController.index(name,age:Int)
#GET     /greet/:name/:lname                           controllers.TestController.index(name,lname)
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
#GET     /                           controllers.Authorization.form
GET     /fetch                              controllers.UserController.getUser